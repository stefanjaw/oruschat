<?xml version="1.0"?>
<odoo>
    <record id="res_company_form_inherited_oruschat" model="ir.ui.view">
        <field name="name">res.company.form.inherited.oruschat</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='general_info']" position="after">
                <page string="Oruschat" name="oruschat">
                    <group>
                        <field string = "Oruschat" name="oruschat_key"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
